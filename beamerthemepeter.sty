\AtBeginSection[]
{
  \begin{frame}<beamer>
    \frametitle{}
    {\Huge\secname}
  \end{frame}
}

\AtBeginSubsection[]
{
  \begin{frame}<beamer>
    \frametitle{}
    {\Huge\secname}\\\leavevmode\\
    {\Large\quad\subsecname}
  \end{frame}
}

\AtBeginSubsubsection[]
{
  \begin{frame}<beamer>
    \frametitle{}
    {\Huge\secname}\\\leavevmode\\
    {\Large\quad\subsecname\quad$|$\quad\Large\subsubsecname}
  \end{frame}
}


\everymath{\color{primaryAccentColour}} % Single $, inline math etc.
\everydisplay{\color{secondaryAccentColour}} % Double $$, equation environment etc.

\newenvironment{Column}[1] % To avoid strange behaviour with colours when using the original column environment.
{\begin{column}{#1}
    \normalcolor}
    {\end{column}}
