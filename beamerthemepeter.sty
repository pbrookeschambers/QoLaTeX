\setbeamercolor*{palette sidebar secondary}{fg=secondaryTextColour}
\setbeamercolor*{palette sidebar tertiary}{fg=primaryTextColour}
\setbeamercolor*{palette sidebar quaternary}{fg=secondaryAccentColour}

\setbeamercolor{title}{fg=secondaryTextColour}
\setbeamercolor*{titlelike}{bg=primaryAccentColour, fg = secondaryTextColour}
\setbeamercolor{frametitle}{bg=primaryAccentColour, fg=secondaryTextColour}
\setbeamercolor{frametitle right}{bg=primaryAccentColour}

\setbeamercolor*{caption name}{bg=primaryTextColour,fg=secondaryAccentColour}
\setbeamercolor*{caption}{bg=primaryTextColour,fg=primaryTextColour}

\usefonttheme{serif}
\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{caption}[numbered]

