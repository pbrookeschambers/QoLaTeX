\newcommand{\colourprovide}[2]{\@ifundefinedcolor{#1}{\colorlet{#1}{#2}}{}}      % (*Only defines the new colour if it has not already been defined*)

\newcommand{\DefineColourScheme}[2]{%
  % Takes the name of the scheme and the hex code colours in a comma separated list.
  \DefineArrayVar % We want to create an array using the forartray package so that we can access the hex code for colours by scheme name and colour name
  {#1}%
  {@}%
  {,}%
  {ForegroundColour,BackgroundColour,Accent1,Accent2,Accent3,Accent4,Accent5,Accent6,Hyperlink,FollowedHyperlink}%
  {,}%
  {#2}  % Since colours should be comma separated already, we don't need to do any additional processing and just pass the whole string to \DefineArrayVar
}

% Quickly grab the red, green, and blue values from a hexstring. Note that this takes the 6-character string as 6 arguments, so it can cause issues with expandable arguments.
\newcommand\getRed[6]{#1#2}
\newcommand\getGreen[6]{#3#4}
\newcommand\getBlue[6]{#5#6}


    \newdimen\rgbR
    \newdimen\rgbG
    \newdimen\rgbB
    \newdimen\hslH
    \newdimen\hslL
    \newdimen\hslS
    \newdimen\hslXmin
    \newdimen\hslV
    \newdimen\hslC
    \newdimen\fA
    \newdimen\fK
    \newdimen\ftemp
    \newdimen\fval
    \newdimen\lightenP
    \newdimen\darkenP



\newcommand{\rgbToHSL}[1]{
%Convert an rgb hex string (*MUST BE IN CAPITALS*) to hsl, storing values in the dimensions \hslH, \hslS, and \hslL
    \rgbR = \fpeval{\number\numexpr"\expandafter\getRed#1\relax / 255}pt
    \rgbG = \fpeval{\number\numexpr"\expandafter\getGreen#1\relax / 255}pt
    \rgbB = \fpeval{\number\numexpr"\expandafter\getBlue#1\relax / 255}pt
    \hslH=0pt
    \hslV=\fpeval{max(\rgbR, \rgbG, \rgbB)} pt
    \hslXmin=\fpeval{min(\rgbR, \rgbG, \rgbB)}pt
    \hslC = \fpeval{\hslV - \hslXmin} pt
    \hslL = \fpeval{(\hslV + \hslXmin) / 2} pt
    \ifdim\hslC<0.0001pt\relax
        \hslH = 0pt
    \else
        \ifdim \hslV = \rgbR
            \hslH = \fpeval{60 * (\rgbG - \rgbB) / \hslC}pt
        \else
            \ifdim \hslV = \rgbG
                \hslH = \fpeval{60 * (2 + (\rgbB - \rgbR) / \hslC)}pt
            \else
                \ifdim \hslV = \rgbB
                    \hslH = \fpeval{60 * (4 + (\rgbR - \rgbG) / \hslC)}pt
                \fi
            \fi
        \fi
    \fi
    \ifdim\hslL<0.0001pt\relax
        \hslS=0pt
    \else
        \ifdim\hslL>0.999pt\relax
            \hslS=0pt
        \else
            \hslS = \fpeval{(\hslV - \hslL) / min(\hslL, 1 - \hslL)} pt
        \fi
    \fi
}

\newcommand{\f}[1]{
\fA = \fpeval{\hslS * min(\hslL, 1 - \hslL)}pt
\ftemp = \fpeval{#1 + \hslH / 30} pt
\fK = \fpeval{\ftemp - trunc(\ftemp / 12) * 12} pt
    \fval = \fpeval{(\hslL - \fA * max(-1, min(\fK - 3, 9 - \fK, 1))) * 255} pt
}

\newcommand{\hslToRGB}{
%Convert the *current* values of \hslH, \hslS, \hslL  to rgb values in the range [0, 255], storing in the dimensions \rgbR, \rgbG, and \rgbB
    \f{0}
    \rgbR=\fval
    \f{8}
    \rgbG=\fval
    \f{4}
    \rgbB=\fval
}

\newcommand{\lighten}[3]{
    % Lightens a colour in a hex string by a certain amount using the hsl model, leaving hue and saturation unchanged, and assigning the new colour a name.
    %
    % #1 - original colour as a hex rgb string
    % #2 - percentage to lighten by
    % #3 - new colour name
    \rgbToHSL{#1}
    \lightenP=\fpeval{#2 / 100}pt
    \hslL = \fpeval{1 - (1 - \hslL)(1 - \lightenP)}pt
    \hslToRGB
    \definecolor{#3}{RGB}{\fpeval{trunc(\rgbR)}, \fpeval{trunc(\rgbG)}, \fpeval{trunc(\rgbB)}}
}
\newcommand{\darken}[3]{
    % darkens a colour in a hex string by a certain amount using the hsl model, leaving hue and saturation unchanged, and assigning the new colour a name.
    %
    % #1 - original colour as a hex rgb string
    % #2 - percentage to lighten by
    % #3 - new colour name
    \rgbToHSL{#1}
    \darkenP=\fpeval{#2 / 100}pt
    \hslL = \fpeval{\hslL*(1-\darkenP)}pt
    \hslToRGB
    \definecolor{#3}{RGB}{\fpeval{trunc(\rgbR)}, \fpeval{trunc(\rgbG)}, \fpeval{trunc(\rgbB)}}
}


