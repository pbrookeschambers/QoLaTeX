%{-- Light/Dark Variants =======================================================
% Light and dark variants swap foreground and background colours, and reverse order of lightening and darkening each colour


\newcommand{\UseColourScheme}[1]{%
\ifcolop@light
% ====================================================================
% If we are using a light sceme
% ====================================================================
\ForEach % Loop through all scheme colours, since all are treated the same except for name
  {,}%
  {%
    \definecolor{\thislevelitem}{HTML}{\csname #1@\thislevelitem\endcsname}% Define the root colour
    \expandafter\rgbToHSL\expandafter{\csname #1@\thislevelitem\endcsname}  % Get its lightness
    \ifthenelse{
        \lengthtest{\hslL < 0.2 pt}
        % If the colour is too dark to have darker variants:
    }{
      % Only produce lighter variants
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{10}{\thislevelitem_1}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{25}{\thislevelitem_2}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{50}{\thislevelitem_3}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{75}{\thislevelitem_4}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{90}{\thislevelitem_5}
    }{
        \ifthenelse{
            \lengthtest{\hslL > 0.8 pt}
            % If colour is too light to have lighter variants:
        }
        {
            % Only produce darker variants
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{90}{\thislevelitem_1}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{75}{\thislevelitem_2}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{50}{\thislevelitem_3}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{25}{\thislevelitem_4}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{10}{\thislevelitem_5}
        }{
            % Otherwise produce 2 darker and 3 lighter variants:
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{50}{\thislevelitem_1}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{25}{\thislevelitem_2}
            \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{40}{\thislevelitem_3}
            \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{60}{\thislevelitem_4}
            \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{80}{\thislevelitem_5}
        }
    }
  }
  {ForegroundColour,BackgroundColour,Accent1,Accent2,Accent3,Accent4,Accent5,Accent6,Hyperlink,FollowedHyperlink}
\else
% ====================================================================
% If we are using a dark sceme
% ====================================================================
\ForEach%
  {,}%
  {%
    \definecolor{\thislevelitem}{HTML}{\csname #1@\thislevelitem\endcsname}%
    \expandafter\rgbToHSL\expandafter{\csname #1@\thislevelitem\endcsname}
    \ifthenelse{
        \lengthtest{\hslL < 0.2 pt}
        % If the colour is too dark to have darker variants:
    }{
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{90}{\thislevelitem_1}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{75}{\thislevelitem_2}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{50}{\thislevelitem_3}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{25}{\thislevelitem_4}
      \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{10}{\thislevelitem_5}
    }{
        \ifthenelse{
            \lengthtest{\hslL > 0.8 pt}
            % If colour is too light to have lighter variants:
        }
        {
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{10}{\thislevelitem_1}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{25}{\thislevelitem_2}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{50}{\thislevelitem_3}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{75}{\thislevelitem_4}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{90}{\thislevelitem_5}
        }{
            % Otherwise produce 2 lighter and 3 darker variants:
            \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{50}{\thislevelitem_1}
            \expandafter\lighten\expandafter{\csname #1@\thislevelitem\endcsname}{25}{\thislevelitem_2}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{40}{\thislevelitem_3}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{60}{\thislevelitem_4}
            \expandafter\darken\expandafter{\csname #1@\thislevelitem\endcsname}{80}{\thislevelitem_5}
        }
    }
  }
  {ForegroundColour, BackgroundColour, Accent1,Accent2,Accent3,Accent4,Accent5,Accent6,Hyperlink,FollowedHyperlink}
  % Swap foreground and background colours
  \colorlet{temp}{BackgroundColour}
  \colorlet{BackgroundColour}{ForegroundColour}
  \colorlet{ForegroundColour}{temp}
  % Swap their variants
  \ForEach{,}{%
    \colorlet{temp_\thislevelitem}{BackgroundColour_\thislevelitem}%
    \colorlet{BackgroundColour_\thislevelitem}{ForegroundColour_\thislevelitem}%
    \colorlet{ForegroundColour_\thislevelitem}{temp_\thislevelitem}%
    }{1,2,3,4,5}
\fi
}
